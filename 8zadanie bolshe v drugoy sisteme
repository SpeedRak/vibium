from itertools import product
s = '0123456789ABCDEF'
k = set()
for x in product(s, repeat=3):
    a = ''.join(x)
    flag = True
    for i in range(1, 3):
        if s.find(a[i-1]) <= s.find(a[i]): #смотрим на алфавит s который расставлен повозрастанию 
            flag = False
    if flag == True:
        k.add(a)
#print(k)
g = set()
for y in product(s, repeat=5):
    b = ''.join(y)
    flag = True
    for z in range(1, 5):
        if s.find(b[z-1]) <= s.find(b[z]): #т.к нам нужно строго(>) мы исключаем (<) и (=)
            flag = False
    if flag == True:
        g.add(b)
#print(g)
print(len(k) + len(g))
#Сколько существует различных трёхзначных и пятизначных чисел, 
#записанных в шестнадцатеричной системе счисления, 
#в записи которых цифры следуют слева направо в строго убывающем порядке?

        