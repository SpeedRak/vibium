f = open('26_3.txt')
n = int(f.readline())
a = {}
for i in range(n):
    rad, mesto = map(int, f.readline().split())
    if rad in a:
        a[rad] += [mesto]
    else:
        a[rad] = [mesto]

xam, kol = 0, 0
for j in sorted(a.keys()): #nomera rada
    b = sorted(a[j]) #spisok
    print(j, b)
    for o in range(1, len(b) - 1):
        if b[o] - b[o-1] == 6 and b[o+1] - b[o] == 6:
            kol += 1
            xam = j
print(xam, kol)

    


