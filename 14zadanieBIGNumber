def f(s):
    res = 0
    n = 0
    for y in s[::-1]:
        res += int(y, 36) * 100**n
        n += 1
    return res

for x in range(100):
    a = f('7a00123') + x * 100**4
    b = f('1ba640') + x * 100 ** 0
    c = f('098012c') + x * 100 ** 6
    if (a - b + c) % 21 == 0:
        print(x, (a - b + c) // 21)
#Максимальное значение х переводим в 6 систему счисления 88(10) > ?(6)
x = 88
y = ''
while x > 0:
    y = str(x%6) + y
    x = x // 6
y 
print(y)